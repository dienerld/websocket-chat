{
  "name": "fastify-websocket",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file .env.local src/index.ts",
    "seed": "tsx --env-file .env.local src/db/seed.ts",
    "build": "tsc --noEmit && tsup",
    "start": "node dist/index.cjs",
    "local:start:production": "node --env-file .env.production dist/index.cjs",
    "local:start:development": "node --env-file .env.local dist/index.cjs",
    "drizzle": "node --env-file .env.local node_modules/drizzle-kit/bin.cjs",
    "drizzle:production": "node --env-file .env.production node_modules/drizzle-kit/bin.cjs",
    "db:push": "pnpm run drizzle push",
    "db:push:production": "pnpm run drizzle:production push",
    "db:studio": "pnpm run drizzle studio",
    "db:generate": "pnpm run drizzle generate",
    "db:generate:push": "pnpm run drizzle generate && pnpm run drizzle push"
  },
  "keywords": [],
  "author": "Diener Dornelas<diener@dienerld.dev>",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/autoload": "^6.2.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "11.0.1",
    "@fastify/session": "^11.1.0",
    "@fastify/swagger": "^9.4.2",
    "@libsql/client": "^0.15.1",
    "@paralleldrive/cuid2": "2.2.2",
    "@scalar/fastify-api-reference": "^1.28.9",
    "drizzle-orm": "0.41.0",
    "fastify": "5.2.2",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "4.0.2",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "22.13.14",
    "drizzle-kit": "0.30.6",
    "tsup": "^8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.7.0"
}
